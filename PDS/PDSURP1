//SBGOLOBL  JOB (ACCT#),S-GOLOB,
// NOTIFY=&SYSUID,
// CLASS=B,MSGCLASS=X
//* ------------------------------------------------- *//
//* RELOAD THE WATFIV AWS TAPE FILES TO DISK DATASETS *//
//* ------------------------------------------------- *//
//GETMOVE  PROC TAPPFX=WATFIV,DATSET=XXXX,NUM=1,DSKPFX=IBMUSER
//STEPM    EXEC PGM=PDSUR
//STEPLIB  DD DSN=SYS1.W$$.LINKLIB,DISP=SHR
//SYSPRINT DD SYSOUT=*
//IN       DD DISP=SHR,DSN=&TAPPFX..&DATSET,UNIT=562,
//         VOL=(,RETAIN,SER=PFG001),LABEL=(&NUM,SL,EXPDT=98000)
//OUT      DD DSN=&DSKPFX..&TAPPFX..&DATSET,DISP=(NEW,CATLG,DELETE),
//         SPACE=(TRK,(45,135,44),RLSE),VOL=SER=WORKE2,UNIT=SYSALLDA
//SYSIN    DD DISP=SHR,DSN=SBGOLOB.B.CNTL(PDSURCTL)
//         PEND
//*
//GENERIT  PROC TAPPFX=WATFIV,DATSET=XXXX,NUM=1,DSKPFX=IBMUSER
//STEPG    EXEC PGM=IEBGENER
//SYSUT1   DD DISP=SHR,DSN=&TAPPFX..&DATSET,UNIT=562,
//         VOL=(,RETAIN,SER=PFG001),LABEL=(&NUM,SL,EXPDT=98000)
//SYSUT2   DD DSN=&DSKPFX..&TAPPFX..&DATSET,DISP=(NEW,CATLG,DELETE),
//            SPACE=(TRK,(45,135),RLSE),VOL=SER=WORKE2,UNIT=SYSALLDA
//SYSPRINT DD SYSOUT=*
//SYSIN    DD DUMMY
//         PEND
//FILE01   EXEC GETMOVE,NUM=01,DATSET=PROCLIB
//FILE02   EXEC GETMOVE,NUM=02,DATSET=JOBLIB
//FILE03   EXEC GETMOVE,NUM=03,DATSET=FUNLIB
//FILE04   EXEC GENERIT,NUM=04,DATSET=TESTS
//FILE05   EXEC GETMOVE,NUM=05,DATSET=WATLIB
//FILE06   EXEC GENERIT,NUM=06,DATSET=MESSAGE
//FILE07   EXEC GETMOVE,NUM=07,DATSET=SOURCE
//FILE08   EXEC GETMOVE,NUM=08,DATSET=MACLIB
//FILE09   EXEC GETMOVE,NUM=09,DATSET=OBJLIB
//FILE10   EXEC GENERIT,NUM=10,DATSET='USER.GUIDE'
//FILE11   EXEC GENERIT,NUM=11,DATSET=ERRTEXTS
//FILE12   EXEC GETMOVE,NUM=12,DATSET=PLOTLIB
