//*    ---------------------------------------------------------------
//*    THIS MEMBER IS TO BE USED WITH THE AWSTAPE, CONVERTED TO A
//*    REAL TAPE, OR RUN UNDER HERCULES.  TO CONVERT TO A REAL TAPE,
//*    YOU CAN USE PROGRAM VTT2TAPE FROM CBT FILE 533. WWW.CBTTAPE.ORG
//*    ---------------------------------------------------------------
//RELOAD  JOB (SYS),'LOAD WATFIV',CLASS=S,MSGCLASS=X
//IEHMOVE  EXEC PGM=IEHMOVE,REGION=4096K
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=3350,VOL=SER=WORK02,DISP=OLD
//IN1      DD  UNIT=TAPE,VOL=(PRIVATE,RETAIN,SER=PFG001),
//             DSN='WATFIV.PROCLIB',LABEL=(1,SL),DISP=(OLD,PASS)
//OUT      DD  UNIT=3350,VOL=SER=PUB001,DISP=OLD   <== TARGET VOLUME
//SYSIN    DD  *
  COPY PDS=WATFIV.PROCLIB,FROM=TAPE=(PFG001,1),                        C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.PROCLIB,CATLG
  COPY PDS=WATFIV.JOBLIB,FROM=TAPE=(PFG001,2),                         C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.JOBLIB,CATLG
  COPY PDS=WATFIV.FUNLIB,FROM=TAPE=(PFG001,3),                         C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.FUNLIB,CATLG
  COPY DSNAME=WATFIV.TESTS,FROM=TAPE=(PFG001,4),                       C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.TESTS,CATLG
  COPY PDS=WATFIV.WATLIB,FROM=TAPE=(PFG001,5),                         C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.WATLIB,CATLG
  COPY DSNAME=WATFIV.MESSAGE,FROM=TAPE=(PFG001,6),                     C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.MESSAGE,CATLG
  COPY PDS=WATFIV.SOURCE,FROM=TAPE=(PFG001,7),                         C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.SOURCE,CATLG
  COPY PDS=WATFIV.MACLIB,FROM=TAPE=(PFG001,8),                         C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.MACLIB,CATLG
  COPY PDS=WATFIV.OBJLIB,FROM=TAPE=(PFG001,9),                         C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.OBJLIB,CATLG
  COPY DSNAME=WATFIV.USER.GUIDE,FROM=TAPE=(PFG001,10),                 C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.USER.GUIDE,CATLG
  COPY DSNAME=WATFIV.ERRTEXTS,FROM=TAPE=(PFG001,11),                   C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.ERRTEXTS,CATLG
  COPY PDS=WATFIV.PLOTLIB,FROM=TAPE=(PFG001,12),                       C
               TO=3350=PUB001,RENAME=SYSO.WATFIV.PLOTLIB,CATLG
//
